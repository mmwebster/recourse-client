<div class="degree-path-insert-component">

  <div id="form" class="layout-column form">

    {{#paper-select
      placeholder=placeholderText
      value=itemToAdd
      onChange=(action (mut itemToAdd))
    }}
      {{#each items as |item|}}
        {{#paper-option value=item.title}}
          {{item.title}}
        {{/paper-option}}
      {{/each}}
    {{/paper-select}}

  </div>

</div>
