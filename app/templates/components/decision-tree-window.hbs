{{#if showDecisionTreeDialog}}
  {{#paper-dialog fullscreen=fullscreen onClose=(action "closeDecisionTreeDialog" "cancel") origin=dialogOrigin}}

    {{#paper-dialog-content}}
      <h2 class="md-title">Please resolve your decision tree</h2>
      {{decision-tree-editor tree=timeline.tree}}
    {{/paper-dialog-content}}

    {{#paper-dialog-actions class="layout-row"}}
      <span class="flex"></span>
      {{#paper-button primary=true warn=true onClick=(action "closeDecisionTreeDialog" "confirm")}}Confirm{{/paper-button}}
    {{/paper-dialog-actions}}

  {{/paper-dialog}}
{{/if}}
